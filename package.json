{
  "name": "council-compass",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://tituomin.github.io/council-compass",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "tachyons": "^4.7.1",
    "tachyons-cli": "^1.0.11"
  },
  "devDependencies": {
    "gh-pages": "^0.12.0",
    "react-scripts": "^0.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "css": "tachyons src/css/app.css > src/index.css --minify",
    "prestart": "npm run css",
    "prebuild": "npm run css",
    "predeploy": "npm run build && mkdir build/static/data; cp importer/cases.json build/static/data",
    "deploy": "gh-pages -d build"
  }
}
